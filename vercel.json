{
    "version": 2,
    "builds": [{
        "src": "PersonalWeb/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.10.4" }
    },
      {
        "src": "builds_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir" : "staticfiles_build"
        }


      }

    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "PersonalWeb/wsgi.py"
        },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"

      }
    ]
}
